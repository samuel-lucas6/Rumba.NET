<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Version>1.0.0</Version>
        <Authors>Samuel Lucas</Authors>
        <Description>A .NET implementation of Rumba20, Rumba12, and Rumba8.</Description>
        <Copyright>Copyright (c) 2023 Samuel Lucas</Copyright>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/samuel-lucas6/Rumba.NET</PackageProjectUrl>
        <RepositoryUrl>https://github.com/samuel-lucas6/Rumba.NET</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>rumba20 rumba12 rumba8 compression-function collision-resistant</PackageTags>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    </PropertyGroup>

</Project>
